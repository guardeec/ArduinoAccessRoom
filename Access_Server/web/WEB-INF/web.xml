<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <servlet>
        <servlet-name>Arduino_GetAcces</servlet-name>
        <servlet-class>Servlets.Arduino_Client.Arduino_GetAccess</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Add_User</servlet-name>
        <servlet-class>Servlets.Admin_Client.Add_User</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Change_User</servlet-name>
        <servlet-class>Servlets.Admin_Client.Change_User</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Delete_User</servlet-name>
        <servlet-class>Servlets.Admin_Client.Delete_User</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Get_User</servlet-name>
        <servlet-class>Servlets.Admin_Client.Get_User</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>Arduino_GetAcces</servlet-name>
        <url-pattern>/Acces</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Add_User</servlet-name>
        <url-pattern>/Add</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Change_User</servlet-name>
        <url-pattern>/Change</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Delete_User</servlet-name>
        <url-pattern>/Delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Get_User</servlet-name>
        <url-pattern>/Get</url-pattern>
    </servlet-mapping>









</web-app>